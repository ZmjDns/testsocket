<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_socket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="opensocket"/>
    <Button
        android:id="@+id/btn_socket1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="openSocket1"/>

    <Button
        android:id="@+id/btn_socketPlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="socketplus"/>

    <Button
        android:id="@+id/btn_objInSocket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="objinsocket"/>

    <Button
        android:id="@+id/btn_mySocket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="mysocket"/>


</LinearLayout>